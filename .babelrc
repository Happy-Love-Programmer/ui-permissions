{
  "env": {
    "development": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "useBuiltIns": "entry",
            "corejs": 3
          }
        ],
        "@babel/preset-react",
        "@babel/preset-flow"
      ],
      "plugins": ["@babel/plugin-proposal-class-properties"]
    },
    "test": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "useBuiltIns": "entry",
            "corejs": 3,
            "modules": "commonjs"
          }
        ],
        "@babel/preset-react"
      ],

      "plugins": ["@babel/plugin-proposal-class-properties"]
    },
    "production": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "useBuiltIns": "entry",
            "corejs": 3
          }
        ],
        "@babel/preset-react",
        "@babel/preset-flow"
      ],
      "plugins": ["@babel/plugin-proposal-class-properties"]
    }
  }
}
